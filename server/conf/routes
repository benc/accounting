GET           /                         controllers.Application.index

GET           /api/expense              controllers.Expenses.list
GET           /api/expense/:id          controllers.Expenses.show(id: String)
DELETE        /api/expense/:id          controllers.Expenses.delete(id: String)
PUT           /api/expense/:id          controllers.Expenses.save(id: String)
POST          /api/expense/:id          controllers.Expenses.add(id: String)

# Map static resources from the /public folder to the /assets URL path
GET           /assets/*file             controllers.Assets.versioned(path="/public", file: Asset)
