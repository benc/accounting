GET           /                       controllers.Application.index

GET           /api/expense            controllers.Expenses.list
GET           /api/expense/:id        controllers.Expenses.show(id: Long)
DELETE        /api/expense/:id        controllers.Expenses.delete(id: Long)
#GET         /api/expense/new        controllers.Expenses.newInvoice
#POST        /api/expense            controllers.Expenses.save

# Map static resources from the /public folder to the /assets URL path
GET           /assets/*file           controllers.Assets.versioned(path="/public", file: Asset)
