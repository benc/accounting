{
  "name": "accounting",
  "version": "42.0.0",
  "description": "Accounting client, an Angular 2 playground since the Angular 2 alpha days",
  "scripts": {
    "typings-install": "typings install",
    "bower-install": "bower install; bower prune",
    "postinstall": "npm run typings-install; npm run bower-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dashboard -- webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 9000 --content-base src",
    "start": "npm run server"
  },
  "license": "MIT",
  "readme": "../README.md",
  "devDependencies": {
    "angular2-template-loader": "0.4.0",
    "awesome-typescript-loader": "1.1.1",
    "css-loader": "0.23.1",
    "raw-loader": "0.5.1",
    "to-string-loader": "1.1.4",
    "typescript": "1.8.9",
    "typings": "1.0.3",
    "webpack": "1.12.9",
    "webpack-dashboard": "0.1.1",
    "webpack-dev-server": "1.14.0",
    "webpack-merge": "0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/compiler-cli": "0.5.0",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
