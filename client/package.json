{
  "name": "accounting-client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "npm install; npm prune",
    "start": "ng serve --port=4200 --host=0.0.0.0",
    "prebuild": "npm install rimraf; rimraf ../backend/src/main/resources/static; npm install; npm prune; npm run lint",
    "build": "ng build --prod --aot=true --output-path=../backend/src/main/resources/static --base-href=/accounting/",
    "pretest": "npm install; npm prune; npm run lint",
    "test": "ng test --single-run --code-coverage",
    "test-debug": "karma start karma.conf.js --single-run=false --browsers=Chrome --debug",
    "lint": "tslint --project tsconfig.json --exclude \"src/**/*.d.ts\" \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "lint-fix": "npm run lint -- --fix",
    "pree2e": "npm install; npm prune",
    "e2e": "env LANG=en_US.UTF-8 ng e2e",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "gitDir": "../",
    "linters": {
      "client/src/**/*.ts": [
        "./node_modules/.bin/tslint --project tsconfig.json --fix",
        "git add"
      ],
      "client/e2e/**/*.ts": [
        "./node_modules/.bin/tslint --project tsconfig.json --fix",
        "git add"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.0",
    "@angular/cdk": "2.0.0-beta.12",
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/flex-layout": "2.0.0-beta.10",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/material": "2.0.0-beta.12",
    "@angular/platform-browser": "5.0.0",
    "@angular/router": "5.0.0",
    "core-js": "2.5.1",
    "hammerjs": "2.0.8",
    "lodash-es": "4.17.4",
    "material-design-icons": "3.0.1",
    "moment": "2.19.1",
    "moment-es6": "1.0.0",
    "roboto-fontface": "0.8.0",
    "rxjs": "5.5.2",
    "ts-helpers": "1.1.2",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/language-service": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@types/chance": "0.7.35",
    "@types/hammerjs": "2.0.35",
    "@types/jasmine": "2.6.2",
    "@types/jasmine-jquery": "1.5.32",
    "@types/lodash-es": "4.17.0",
    "@types/node": "8.0.47",
    "chance": "1.0.12",
    "codelyzer": "4.0.1",
    "husky": "0.14.3",
    "jasmine-core": "2.8.0",
    "jasmine-jquery": "2.1.1",
    "jasmine-reporters": "2.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-jquery": "0.1.1",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.5",
    "lint-staged": "4.3.0",
    "mkdirp": "0.5.1",
    "prettier": "1.7.4",
    "protractor": "5.2.0",
    "protractor-console": "3.0.0",
    "protractor-console-plugin": "0.1.1",
    "protractor-jasmine2-screenshot-reporter": "0.5.0",
    "rimraf": "2.6.2",
    "rxjs-tslint-rules": "3.0.4",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "tslint-config-prettier": "1.6.0",
    "tslint-defocus": "2.0.3",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "2.5.0"
  },
  "false": {}
}
